<%= link_to "登録画面", new_book_path %>
<h1>書籍マスタ一覧</h1>

<table>
<tr>
    <th>書籍コード</th>
    <th>書籍名</th>
    <th>出版社コード</th>
    <th>送信出版社コード</th>
</tr>

    <% @books.each do |book|%>
<tr>
    <td><%= book.book_code %></td>
    <td><%= book.book_title %></td>
    <td><%= book.publisher_code %></td>
    <td><%= book.sender_publisher_code %></td>
<tr>
<% end %>
</table>