<%= link_to "登録画面", new_book_path %>
<h1>書籍マスタ一覧</h1>

<table>
<tr>
    <th>書籍コード</th>
    <th>書籍名</th>
    <th>出版社コード</th>
    <th>送信出版社コード</th>
</tr>
    <% @books.each do |book|%>
<tr>
    <td>
        <%= link_to edit_book_path(book.id) do %>
        <%= book.book_code %>
        <% end %>
    </td>
        <td>
        <%= link_to edit_book_path(book.id) do %>
        <%= book.book_title %>
        <% end %>
    </td>
        <td>
        <%= link_to edit_book_path(book.id) do %>
        <%= book.publisher_code %>
        <% end %>
    </td>
        <td>
        <%= link_to edit_book_path(book.id) do %>
        <%= book.sender_publisher_code %>
        <% end %>
    </td>
<tr>
    <% end %>
</table>